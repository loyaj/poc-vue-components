    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vitest": "^0.3.22",
    "eslint-plugin-vue": "^9.21.1",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.0.3",
    "vite": "^4.5.3",
    "vitest": "^0.34.6"
  }
}
