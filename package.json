{
  "name": "poc-vue-components",
  "version": "0.2.4",
  "author": "Josh Loya <jloya3@oreillyauto.com>",
  "type": "module",
  "exports": {
    ".": "./dist/poc-vue-components.js",
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore && prettier . -w --check --log-level warn --ignore-path .gitignore",
