{
  "name": "poc-vue-components",
  "version": "0.5.0",
  "author": "Josh Loya <jloya3@oreillyauto.com>",
  "type": "module",
  "exports": {
    "./style.css": "./dist/style.css",
    "./design-tokens.css": "./dist/design-tokens.css",
    "./quasar-variables.scss": "./dist/quasar-variables.scss",
    "./*": "./dist/components/*/*.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore && prettier . -w --check --log-level warn --ignore-path .gitignore",
    "lint:no-fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore --max-warnings=0 && prettier . --check --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "quasar": "^2.17.0"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.7.0",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/addon-interactions": "^7.6.20",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/blocks": "^7.6.20",
    "@storybook/test": "^7.6.20",
    "@storybook/vue3": "^7.6.20",
    "@storybook/vue3-vite": "^7.6.20",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vitest": "^0.3.22",
    "eslint-plugin-vue": "^9.21.1",
    "glob": "^10.4.5",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.70.0",
    "storybook": "^7.6.20",
    "vite": "^4.5.3",
    "vitest": "^0.34.6"
  }
}
